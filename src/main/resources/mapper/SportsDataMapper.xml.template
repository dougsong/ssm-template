<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd" >
<mapper namespace="com.em.smart.mapper.SportsDataMapper">

    <insert id="insertData" parameterType="com.em.smart.pojo.SportsData" >
        INSERT ss_sport_data VALUES(#{id}, #{date}, #{time}, #{ax}, #{ay}, #{az}, #{wx}, #{wy}, #{wz}, #{angleX},
                                      #{angleY}, #{angleZ}, #{temperature}, #{hx}, #{hy}, #{hz})
    </insert>

</mapper>